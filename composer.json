{
  "name": "lcs13761/erede-laravel",
  "description": "Pacote Laravel para integração com a API de pagamentos da eRede (Rede S.A.)",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "erede",
    "payment",
    "pagamento"
  ],
  "homepage": "https://github.com/lcs13761/erede-laravel",
  "authors": [
    {
      "name": "lcs13",
      "email": "lcs13761@hotmail.com",
      "homepage": "https://github.com/lcs13761",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.48|^11.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^8.22|^9.0",
    "pestphp/pest": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Lcs13761\\EredeLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lcs13761\\EredeLaravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lcs13761\\EredeLaravel\\Providers\\EredeServiceProvider"
      ],
      "aliases": {
        "Erede": "Lcs13761\\EredeLaravel\\Facades\\Erede"
      }
    }
  },
  "scripts": {
    "test": "pest",
    "test-coverage": "pest --coverage",
    "test-watch": "pest --watch",
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
